{
    "version": "fbal-r01",
    "pwm_transition_value": 70,
    "pwm_boost_value": 100,
    "sample_interval_ms": 3000,
    "fan_recovery_time": 2,
    "boost": {
      "fan_fail": true,
      "sensor_fail": false
    },
    "sensor_valid_check": {
      "mb": {
        "attribute": {
          "type" : "power_status"
        }
      }
    },  
    "watchdog": false,
    "min_rpm": 1500,
    "profiles": {
      "linear_dimm": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "data": [
          [60, 20],
          [65, 25],
          [70, 30]
        ]
      },
      "linear_mezz": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "positive_hysteresis":1,
        "negative_hysteresis":1,
        "data": [
          [70, 10],
          [75, 20],
          [80, 30],
          [85, 40],
          [90, 50]
        ]
      },
      "linear_inlet": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "data": [
          [20, 10],
          [25, 25],
          [30, 30],
          [35, 35],
          [38, 50],
          [40, 80]
        ]
      },
      "linear_cpu_margin": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "linear",
        "data": [
          [-50, 5],
          [-40, 10],
          [-30, 15],
          [-20, 20],
          [-13, 25]
        ]
      },
      "pid_cpu_margin": {
        "read_source" : {
          "util" : "/usr/local/bin/sensor-util"
        },
        "type": "pid",
        "setpoint": -10,
        "negative_hysteresis": 2,
        "positive_hysteresis": 1,
        "kp": 3.5,
        "ki": 0.4,
        "kd": 0.4
      }
    },
    "fans": {
      "0": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 0"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 0"
        }
      },
      "1": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 1"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 0"
        }
      },
      "2": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 2"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 1"
        }
      },
      "3": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 3"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 1"
        }
      },
      "4": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 4"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 2"
        }
      },
      "5": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 5"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 2"
        }
      },
      "6": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 6"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 3"
        }
      },
      "7": {
        "read_source" : {
          "util" : "/usr/local/bin/fan-util --get 7"
        },
        "write_source" : {
          "util" : "/usr/local/bin/fan-util --set %d 3"
        }
      }
    },
    "zones": {
      "zone_1": {
        "pwm_output": [0, 2, 4, 6],
        "expr_file": "zone1_2s.fsc"
      }
    }
}
  
