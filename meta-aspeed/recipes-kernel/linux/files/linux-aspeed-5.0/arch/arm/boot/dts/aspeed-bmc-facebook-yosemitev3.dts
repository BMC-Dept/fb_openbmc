/dts-v1/;

#include "aspeed-g5.dtsi"
#include <dt-bindings/gpio/aspeed-gpio.h>

/ {
	model = "Facebook YosemiteV3 BMC";
	compatible = "facebook,yosemitev3-bmc", "aspeed,ast2500";
	aliases {
		serial0 = &uart5;
                serial1 = &uart1;
                serial2 = &uart2;
                serial3 = &uart3;
                serial4 = &uart4;
	};
	chosen {
		stdout-path = &uart5;
		bootargs = "debug console=ttyS4,57600n8 root=/dev/ram rw";
	};

	memory@80000000 {
		reg = <0x80000000 0x20000000>;
	};

	iio-hwmon {
		compatible = "iio-hwmon";
		io-channels = <&adc 0>, <&adc 1>, <&adc 2>, <&adc 3>,
			      <&adc 4>, <&adc 5>, <&adc 6>, <&adc 7>;
	};
};

&fmc {
        status = "okay";
        flash@0 {
                reg = < 1 >;
                status = "okay";
                m25p,fast-read;
                label = "spi0.1";
                spi-max-frequency = <12000000>;
#include "facebook-bmc-flash1-layout.dtsi"
        };
        flash@1 {
                reg = < 0 >;
                status = "okay";
                m25p,fast-read;
                label = "spi0.0";
                spi-max-frequency = <12000000>;
#include "facebook-bmc-flash0-layout.dtsi"
        };
};

&ehci0 {
        status = "okay";
};

&ehci1 {
        status = "okay";
};

&spi1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_spi1_default>;
	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "pnor";
	};
};

&ibt {
        status = "disabled";
};

&rtc {
	status = "okay";
};

&uart1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_txd1_default
		     &pinctrl_rxd1_default>;
};

&uart2 {
	status = "okay";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_txd2_default
                     &pinctrl_txd2_default>;
};

&uart3 {
	status = "okay";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_txd3_default
                     &pinctrl_txd3_default>;
};

&uart4 {
        status = "okay";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_txd4_default
                     &pinctrl_txd4_default>;
};

&uart5 {
	status = "okay";
};

&mac0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_rmii1_default>;
	use-ncsi;
};

&adc {
	status = "okay";
};

&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&i2c2 {
	status = "okay";
};

&i2c3 {
	status = "okay";
};

&i2c4 {
	status = "okay";
};

&i2c5 {
	status = "okay";
};

&i2c6 {
	status = "okay";
};

&i2c7 {
	status = "okay";
};

&i2c8 {
	status = "okay";
};

&i2c9 {
	status = "okay";
};

&i2c10 {
        status = "okay";
};

&i2c11 {
        status = "okay";
};

&i2c12 {
        status = "okay";
};

&i2c13 {
        status = "okay";
};

&sdhci_slot1 {
	//Slot Register Set for slot1
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_sd2_default>;
};
