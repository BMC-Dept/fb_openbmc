/dts-v1/;

#include "facebook-netbmc-ast2500-common.dtsi"

/*ELBERTTODO 447406 REVIEW KERNEL IO*/
/ {
	model = "Facebook ELBERT BMC";
	compatible = "facebook,elbert-bmc", "aspeed,ast2500";

	aliases {
		/*
		 * Override the default uart aliases to avoid breaking
		 * the legacy applications.
		 */
		serial0 = &uart5;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;

		spi2 = &spi2;
	};

	chosen {
		stdout-path = &uart5;
		bootargs = "console=ttyS0,9600n8 root=/dev/ram rw";
	};
};

/*ELBERTTODO 447406 REVIEW KERNEL SPI*/
&spi2 {
	status = "okay";
	compatible = "aspeed,ast2500-spi-master";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_spi2ck_default
		     &pinctrl_spi2cs0_default
		     &pinctrl_spi2cs1_default
		     &pinctrl_spi2miso_default
		     &pinctrl_spi2mosi_default>;

	spidev@0 {
		compatible = "rohm,dh2228fv";
		spi-max-frequency = <33000000>;
		reg = <0>;
	};
};

&uart2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_txd2_default
		     &pinctrl_rxd2_default>;
};


&mac0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_rgmii1_default &pinctrl_mdio1_default>;
};

&mac1 {
	status = "disabled";
};

/*ELBERTTODO SCD SMBus MUX*/
&i2c0 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&i2c2 {
	status = "okay";
        /** ELBERTTODO SCD MUX
        i2c-switch@75 {
                compatible = "nxp,pca9548";
                #address-cells = <1>;
                #size-cells = <0>;
                reg = <0x75>;
        };
        **/
};

&i2c3 {
	status = "okay";
};

&i2c4 {
	status = "okay";
};

&i2c5 {
	status = "okay";
        /** ELBERTTODO SCD MUX
        i2c-switch@75 {
                compatible = "nxp,pca9548";
                #address-cells = <1>;
                #size-cells = <0>;
                reg = <0x75>;
        };
        **/
};

&i2c6 {
	status = "okay";
};

&i2c7 {
	status = "okay";
};

&i2c8 {
	status = "okay";
};

&i2c9 {
	status = "okay";
};

&i2c10 {
	status = "okay";
};

&i2c11 {
	status = "okay";
