/dts-v1/;

#if CONFIG_IS_ENABLED(PFR_SUPPORT)
#include "ast2500-fb-pfr.dtsi"
#else
#include "ast2500-fb-vboot.dtsi"
#endif

#if CONFIG_IS_ENABLED(TPM_V1)
&i2c9 {
	status = "okay";
	u-boot,dm-pre-reloc;
	tpm@20 {
		/* Emulators should use atmel,at97sc3204t. */
		compatible = "infineon,slb9645tt";
		reg = <0x20>;
		clocks = <&scu ASPEED_CLK_APB>;
		u-boot,dm-pre-reloc;
	};
};
#endif

#if CONFIG_IS_ENABLED(TPM_V2)
#include <dt-bindings/gpio/aspeed-gpio.h>
/ {
	spi_gpio: spi-gpio {
		status = "okay";
		compatible = "spi-gpio";
		#address-cells = <1>;
		#size-cells = <0>;

		gpio-sck = <&gpio ASPEED_GPIO(R, 3) GPIO_ACTIVE_HIGH>;
		gpio-mosi = <&gpio ASPEED_GPIO(R, 4) GPIO_ACTIVE_HIGH>;
		gpio-miso = <&gpio ASPEED_GPIO(R, 5) GPIO_ACTIVE_HIGH>;
		num-chipselects = <1>;
		cs-gpios = <&gpio ASPEED_GPIO(R, 2) GPIO_ACTIVE_LOW>;

		tpmdev@0 {
			compatible = "tis,tpm2-spi";
			spi-max-frequency = <33000000>;
			reg = <0>;
		};
	};
};
#endif